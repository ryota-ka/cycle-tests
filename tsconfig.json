{
  "compilerOptions": {
    "jsx": "react",
    "jsxFactory": "Snabbdom.createElement",
    "lib": ["dom", "es2015"],
    "keyofStringsOnly": true,
    "module": "CommonJS",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "strictFunctionTypes": false,
    "target": "ESNext"
  }
}
